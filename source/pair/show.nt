import {
  this.box,
  this.list,
  this.pair {Pair, pair},
  this.show {Show, show},
  this.text {intercalate, join},
}

inline as-show<a, b>(s1: show(a), s2: show(b)): show(pair(a, b)) {
  Show of {
    show := {
      function (p) {
        let Pair(x, y) = p;
        let Show of {show := show1} = s1;
        let Show of {show := show2} = s2;
        join(List[
          *"(",
          intercalate(List[show1(x), show2(y)], *", "),
          *")",
        ])
      }
    },
  }
}
