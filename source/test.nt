define with-cell(i: int): int {
  let c = make-cell(lift {i});
  let _ on c =
    mutate(c, function (x) {
      lift {add-int(axiom-T(x), 1)}
    });
  let _ on c =
    mutate(c, function (x) {
      lift {add-int(axiom-T(x), 1)}
    });
  let _ on c =
    mutate(c, function (x) {
      lift {add-int(axiom-T(x), 1)}
    });
  let _ on c =
    mutate(c, function (x) {
      lift {add-int(axiom-T(x), 21)}
    });
  axiom-T(this.cell.extract(c))
}

define box-int(i: int): meta int {
  magic cast(int, meta int, i)
}

define use-rune(r: rune): unit {
  match r {
  | `a` =>
    print("found a\n")
  | `あ` =>
    print("found あ\n")
  | _ =>
    print("found other\n")
  }
}

