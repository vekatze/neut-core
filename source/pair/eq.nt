import {
  this.bool {and},
  this.eq {Eq, eq},
  this.pair {Pair, pair},
}

inline as-eq<a, b>(f: eq(a), g: eq(b)): eq(pair(a, b)) {
  let Eq{equal := equal-f} = f;
  let Eq{equal := equal-g} = g;
  Eq{
    equal := {
      function (p1, p2) {
        tie Pair(x1, y1) = p1;
        tie Pair(x2, y2) = p2;
        and(equal-f(x1, x2), equal-g(y1, y2))
      }
    },
  }
}
