data bool {
| False
| True
}

inline and(b1: bool, b2: bool): bool {
  if b1 {
    if b2 {
      True
    } else {
      False
    }
  } else {
    False
  }
}

inline or(b1: bool, b2: bool): bool {
  if b1 {
    True
  } else {
    if b2 {
      True
    } else {
      False
    }
  }
}

inline not(b: bool): bool {
  if b {
    False
  } else {
    True
  }
}

rule-left any {
  function (acc: bool, b: bool): bool {
    or(acc, b)
  },
  function (): bool {
    False
  },
}

rule-left all {
  function (acc: bool, b: bool): bool {
    and(acc, b)
  },
  function (): bool {
    True
  },
}
