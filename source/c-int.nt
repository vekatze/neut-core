import {
  this.bool {bool},
}

alias _c-int {
  introspect architecture {
  | amd64 =>
    int32
  | arm64 =>
    int32
  }
}

data c-int {
| C-Int(int32)
}

inline add-c-int(x: c-int, y: c-int): c-int {
  let C-Int(x) = x;
  let C-Int(y) = y;
  introspect architecture {
  | amd64 =>
    C-Int(add-int32(x, y))
  | arm64 =>
    C-Int(add-int32(x, y))
  }
}

inline sub-c-int(x: c-int, y: c-int): c-int {
  let C-Int(x) = x;
