import {
  this.either {Left, Right, either},
  this.list,
  this.show {Show, show},
  this.text {format},
}

inline as-show<a, b>(s1: show(a), s2: show(b)): show(either(a, b)) {
  Show(function (val) {
    match val {
    | Left(x) =>
      let Show of {show} = s1 in
      format("Left({})", [show(x)])
    | Right(y) =>
      let Show of {show} = s2 in
      format("Right({})", [show(y)])
    }
  })
}
