define increment(x: int): int {
  add-int(x, 1)
}

define is-even(x: int): bool {
  eq-int(rem-int(x, 2), 0)
}

inline loop<a>(xs: list(a)): list(a) {
  match xs {
  | Nil =>
    Nil
  | Cons(y, ys) =>
    Cons(y, loop(ys))
  }
}

