import {
  this.bool {False, True, bool},
  this.ord {Ord, ord},
  this.ordering {EQ},
}

data eq(a) {
| Eq(
    equal: (&a, &a) -> bool,
  )
}

inline from-ord<a>(o: ord(a)): eq(a) {
  Eq{
    equal := {
      function (x, y) {
        let Ord{compare} = o;
        match compare(x, y) {
        | EQ =>
          True
        | _ =>
          False
        }
      }
    },
  }
}
