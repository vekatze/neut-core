import {
  this.bool {False},
  this.either {Left, Right, either},
  this.eq {Eq, eq},
}

inline as-eq<a, b>(f: eq(a), g: eq(b)): eq(either(a, b)) {
  Eq{
    equal := {
      function (p1, p2) {
        case p1, p2 {
        | Left(l1), Left(l2) =>
          let Eq{equal} = f;
          equal(l1, l2)
        | Right(r1), Right(r2) =>
          let Eq{equal} = g;
          equal(r1, r2)
        | _, _ =>
          False
        }
      }
    },
  }
}
