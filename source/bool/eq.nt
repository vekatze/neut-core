import {
  this.bool {False, True, bool, not},
  this.box,
  this.eq {Eq, eq},
}

inline as-eq: eq(bool) {
  Eq(
    function (b1, b2) {
      if *b1 {
        *b2
      } else {
        not(*b2)
      }
    },
  )
}

define eq-bool(b1: bool, b2: bool): bool {
  match b1, b2 {
  | True, True =>
    True
  | False, False =>
    True
  | _, _ =>
    False
  }
}
