import {
  this.bool {False, True, bool},
  this.eq {eq},
  this.list {Cons, Nil, list},
}

inline _elem<a>(!e: eq(&a)): (xs: &list(a), x: &a) -> bool {
  define self(xs: &list(a), x: &a): bool {
    case xs {
    | Nil =>
      False
    | Cons(y, ys) =>
      if !e::equal(x, y) {
        True
      } else {
        self(ys, x)
      }
    }
  }
}

data trope(a) {
| Trope(
    elem: (xs: &list(a), x: &a) -> bool,
  )
}

inline from-eq<a>(e: eq(&a)): trope(a) {
  Trope of {
    elem = _elem(e),
  }
}
