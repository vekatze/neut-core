import {
  this.bool {bool},
  this.box,
  this.eq {Eq, eq},
  this.rune {to-int32},
}

define eq-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  eq-int32(r1, r2)
}

define ne-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  ne-int32(r1, r2)
}

inline as-eq: eq(rune) {
  Eq of {
    equal = eq-rune,
  }
}

inline as-eq-N: eq(&rune) {
  Eq of {
    equal = {
      function (x, y) {
        as-eq::equal(*x, *y)
      }
    },
  }
}
